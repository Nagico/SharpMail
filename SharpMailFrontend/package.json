{
  "name": "sharp_mail_frontend",
  "private": true,
  "main": "main.js",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "electron:start": "set NODE_ENV=electron_dev&& vite build & electron .",
    "electron:build": "set NODE_ENV=electron_build&&electron-builder",
    "lint": "eslint src/**/*.{js,vue} --fix",
    "format": "prettier --write 'src/**/*.{js,vue}'"
  },
  "dependencies": {
    "@vueuse/core": "^8.9.4",
    "axios": "^0.27.2",
    "element-plus": "^2.2.10",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2",
    "vue3-quill": "^0.2.6"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.4",
    "@vitejs/plugin-vue": "^3.0.0",
    "electron": "^19.0.9",
    "electron-builder": "^23.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "less": "^4.0.0",
    "prettier": "^2.7.1",
    "quill": "^1.3.7",
    "unplugin-auto-import": "^0.8.2",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^3.0.0",
    "vite-plugin-eslint": "^1.6.1"
  },
  "build": {
    "extraResources": [
      {
        "from": "./server/",
        "to": "server"
      }
    ],
    "files": [
      "*.js",
      "dist",
      "node_modules"
    ]
  }
}